<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/MainBackgraund"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.splash.SplashScreenFragment">


    <ImageView
        android:id="@+id/iconStar"
        android:layout_width="117dp"
        android:layout_height="112dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/ic_star_1"/>

    <ImageView
        android:id="@+id/tvMomentum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_vector"
        android:layout_marginTop="41dp"
        app:layout_constraintTop_toBottomOf="@id/iconStar"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:layout_width="15.18dp"
        android:layout_height="4.33dp"
        android:layout_marginTop="41dp"
        app:layout_constraintTop_toBottomOf="@id/iconStar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:src="@drawable/ic_vector_one"
        android:layout_marginRight="15dp"/>

    <ImageView
        android:layout_width="15.18dp"
        android:layout_height="4.33dp"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/iconStar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:src="@drawable/ic_vector_two"
        android:layout_marginRight="15dp"/>

    <ImageView
        android:layout_width="15.18dp"
        android:layout_height="4.33dp"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@id/iconStar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:src="@drawable/ic_vector_three"
        android:layout_marginRight="15dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>